   "source": [
    "def dic_unpack(data):\n",
    "    for col in ['genres', 'keywords', 'production_companies', 'production_countries', 'spoken_languages']:\n",
    "        data[col] = [i.get('name', 'None') for i in json.loads(data[col])]\n",
    "        data[col] = \" \".join(data[col])\n",
    "    return data\n",
    "           "
   ]
